<footer class="py-5 bg-footer xs-mt-m10">
    <div class="container">
        <div class="row">
            <div class="col-12 col-sm-3 d-sm-block xs-mb5 wrap-footer-logo">
                <a href="{{ url('home') }}" class="link-footer-logo">
                    <img src="{{ asset('assets/images/logo_footer.png') }}" alt="Promotip" class="img-fluid">
                </a>
            </div>
            <div class="col-12 col-sm-7 offset-sm-2 pt-sm-2 fw-300">
                <div class="row">
                    <div class="col-12 col-sm-4 xs-mb-half">
                        <h6 class="text-white xs-text-center xs-mb-1">Populaire zoektermen</h6>
                        <ul class="list-unstyled xs-row">
                            {{ render(url('getMostUsedKeywordsFooter')) }}
                        </ul>
                    </div>
                    <div class="col-12 col-sm-4">
                        <h6 class="text-white xs-text-center xs-mb-1"><a href="{{ url('company_index') }}"
                                                                         class="text-white">{% trans %}Company guide{% endtrans %}</a></h6>
                        <ul class="list-unstyled xs-row">
                            {% set provinces = provinces.get %}
                            {% for province in provinces %}
                                {% if province.regionLanguage == app.request.locale|upper %}
                                    <li class="xs-half footer-links"><a href="{{ url("company_province", {'province' : province.nameDirify}) }}"
                                                                        class="text-white">{{ province.name }}</a></li>
                                {% endif %}
                            {% endfor %}
                        </ul>
                    </div>
                    <div class="col-12 col-sm-4 xs-mt-moins40">
                        <br>
                        <ul class="list-unstyled xs-row">
                            {% for province in provinces %}
                                {% if province.regionLanguage != app.request.locale|upper %}
                                    <li class="xs-half footer-links"><a href="{{ url("company_province", {'province' : province.nameDirify}) }}"
                                                                        class="text-white">{{ province.name }}</a></li>
                                {% endif %}
                            {% endfor %}
                        </ul>
                    </div>
                </div>

            </div>
        </div>
        <div class="row mt-5 text-white fw-300">
            <div class="col-12 col-sm-4 text-center text-sm-left mb-2 mb-sm-0 footer-bottom-links xs-order-1">
                <a href="{{ url('terms') }}" class="text-white">{% trans %}Terms{% endtrans %}</a>
                |
                <a href="{{ url('cookies') }}" class="text-white">{% trans %}Cookies{% endtrans %}</a>
                |
                <a href="{{ url('privacy') }}" class="text-white">Privacybeleid</a>
                |
                <a href="{{ url('faq') }}" class="text-white">{% trans %}FAQ{% endtrans %}</a>
                |
                <a href="{{ url('advertise') }}" class="text-white">Adverteren</a>
                |
                <a href="https://www.promo-tip.nl" class="text-white">{% trans %}Promotip Nederland{% endtrans %}</a>
            </div>
            <div class="col-12 col-sm-5 text-center xs-order-3">
